package rutas.uacm.models.service.imp;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;

import rutas.uacm.models.document.Ruta;
import rutas.uacm.models.document.RutaParada;
import rutas.uacm.models.repository.IUnidadTransporteRepository;
import rutas.uacm.models.repository.RutaParadaRepository;
import rutas.uacm.models.service.IRutaService;

public class RutaParadaServiceImp implements IRutaService{

	@Autowired
	RutaParadaRepository _rutaParadaRepository;

	@Override
	public List<Ruta> obtenerTodos() {
		return _rutaParadaRepository.findAll();
	}

	@Override
	public Ruta buscarId(String id) {
		return _rutaParadaRepository.findById(id).orElse(null);
	}

	@Override
	public Ruta insertar(Ruta ruta) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public void eliminar(String id) {
		// TODO Auto-generated method stub
		
	}
	


}
